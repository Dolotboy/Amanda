{
  "sites": [
    {
      "name": "YouTube",
      "synonymes": [ "yt", "tube" ],
      "link": "https://www.youtube.com",
      "parameters": [
        {
          "name": "id",
          "prerequisite": "",
          "description": "Go to a specific channel",
          "urlParameter": "/watch?v=~PARAMETER~"
        },
        {
          "name": "time",
          "prerequisite": "id",
          "description": "Choose the time to start the video at (In second)",
          "urlParameter": "&t=~PARAMETER~s"
        },
        {
          "name": "user",
          "prerequisite": "",
          "description": "Go to a specific channel",
          "urlParameter": "/@~PARAMETER~"
        },
        {
          "name": "query",
          "prerequisite": "",
          "description": "Search for videos with the query",
          "urlParameter": "/results?search_query=~PARAMETER~"
        }
      ]
    },
    {
